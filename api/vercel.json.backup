{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "env": {
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_ANON_KEY": "@supabase_anon_key",
        "SUPABASE_SERVICE_KEY": "@supabase_service_key",
        "JWT_SECRET": "@jwt_secret",
        "JWT_EXPIRES_IN": "7d",
        "NODE_ENV": "production",
        "FRONTEND_URL": "@frontend_url"
    }
}