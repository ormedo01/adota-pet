{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "npx vite build",
    "outputDirectory": "dist",
    "installCommand": "npm ci --legacy-peer-deps",
    "framework": null,
    "env": {
        "VITE_API_URL": "@vite_api_url"
    }
}